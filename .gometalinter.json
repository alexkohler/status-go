{
    "Exclude": [
        ".*_mock.go",
        "geth/jail/doc.go"
    ],
    "Skip": ["helpers", "static"],
    "Vendor": true,
    "Linters": {
        "gasv2": {
          "Command": "gas",
          "Pattern": "^(?P<path>.*?\\.go),(?P<line>\\d+),(?P<message>[^,]+,[^,]+,[^,]+)"
        }
      },
    "Enable": [
        "gasv2"
    ],
    "Cyclo": 16
}
